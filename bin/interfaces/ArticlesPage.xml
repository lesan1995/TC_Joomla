<?xml version="1.0" encoding="UTF-8"?>
<Page name="ArticlesPage">
	<Element id="toolBar">
		<By>cssSelector</By>
		<Address>div[id='toolbar-{0}']</Address>	
	</Element>
	<Element id="tbSearch">
		<By>cssSelector</By>
		<Address>input[id*='search']</Address>	
	</Element>
	<Element id="btnSearch">
		<By>cssSelector</By>
		<Address>button>span[class*='search']</Address>	
	</Element>
	<Element id="btnNext">
		<By>cssSelector</By>
		<Address>a[data-original-title='Next']</Address>	
	</Element>
	<Element id="btnClear">
		<By>xpath</By>
		<Address>//button[contains(text(),'Clear')]</Address>	
	</Element>
	<Element id="btnSearchTools">
		<By>xpath</By>
		<Address>//button[contains(.,'Search Tools')]</Address>	
	</Element>
	<Element id="btnStatus">
		<By>xpath</By>
		<Address>//div[@id='filter_published_chzn']/a</Address>	
	</Element>
	<Element id="lbStatus">
		<By>xpath</By>
		<Address>//div[@id='filter_published_chzn']//li[contains(text(),'{0}')]</Address>	
	</Element>
	<Element id="resultSearch">
		<By>cssSelector</By>
		<Address>table>tbody>tr>td</Address>	
	</Element>
	<Element id="cbArticleInTable">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr//input[@type='checkbox']</Address>	
	</Element>
	<Element id="icPublishArticleInTable">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr//span[@class='icon-publish']</Address>	
	</Element>
	<Element id="icUnpublishArticleInTable">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr//span[@class='icon-unpublish']</Address>	
	</Element>
	<Element id="messageArticle">
		<By>cssSelector</By>
		<Address>div[class='alert-message']</Address>	
	</Element>
	<Element id="tagAInTitleTable">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::td//a</Address>	
	</Element>
	<Element id="titleHelpPage">
		<By>cssSelector</By>
		<Address>title</Address>	
	</Element>
	<Element id="btnCategory">
		<By>xpath</By>
		<Address>//div[@id='filter_category_id_chzn']/ul</Address>	
	</Element>
	<Element id="lbCategory">
		<By>xpath</By>
		<Address>//div[@id='filter_category_id_chzn']/div//li[contains(text(),'{0}')]</Address>	
	</Element>
	<Element id="btnAccess">
		<By>xpath</By>
		<Address>//div[@id='filter_access_chzn']/ul</Address>	
	</Element>
	<Element id="lbAccess">
		<By>xpath</By>
		<Address>//div[@id='filter_access_chzn']/div//li[contains(text(),'{0}')]</Address>	
	</Element>
	<Element id="btnAuthor">
		<By>xpath</By>
		<Address>//div[@id='filter_author_id_chzn']/ul</Address>	
	</Element>
	<Element id="lbAuthor">
		<By>xpath</By>
		<Address>//div[@id='filter_author_id_chzn']/div//li[contains(text(),'{0}')]</Address>	
	</Element>
	<Element id="rowInTable">
		<By>xpath</By>
		<Address>//table/tbody/tr</Address>	
	</Element>
	<Element id="icStatusInTable">
		<By>xpath</By>
		<Address>//table/tbody/tr[{0}]/td[count(//table//th[contains(.,'Status')]/preceding-sibling::th)+1]//span[@class='icon-{1}']</Address>	
	</Element>
	<Element id="categoryInTable">
		<By>xpath</By>
		<Address>//table/tbody/tr[{0}]/td[count(//table//th[contains(.,'Title')]/preceding-sibling::th)+1]//div[contains(text(),'Category')]</Address>	
	</Element>
	<Element id="accessInTable">
		<By>xpath</By>
		<Address>//table/tbody/tr[{0}]/td[count(//table//th[contains(.,'Access')]/preceding-sibling::th)+1]</Address>	
	</Element>
	<Element id="authorInTable">
		<By>xpath</By>
		<Address>//table/tbody/tr[{0}]/td[count(//table//th[contains(.,'Author')]/preceding-sibling::th)+1]/a</Address>	
	</Element>
	<Element id="idXpathInTable">
		<By>xpath</By>
		<Address>//table/tbody/tr[{0}]/td[count(//table//th[contains(.,'ID')]/preceding-sibling::th)+1]</Address>	
	</Element>
	<Element id="idInTable">
		<By>cssSelector</By>
		<Address>table>tbody>tr:nth-of-type({0})>td:last-child</Address>	
	</Element>
	<Element id="btnListLimit">
		<By>cssSelector</By>
		<Address>div[id='list_limit_chzn']>a</Address>	
	</Element>
	<Element id="lbListLimit">
		<By>xpath</By>
		<Address>//div[@id='list_limit_chzn']/div//li[contains(text(),'{0}')]</Address>	
	</Element>
	<Element id="linkIDColumn">
		<By>xpath</By>
		<Address>//table//th[contains(.,'ID')]/a</Address>	
	</Element>
	<Element id="btnBackWard">
		<By>xpath</By>
		<Address>//ul[@class='pagination-list']//a[contains(@data-original-title,'Start')]</Address>	
	</Element>
	<Element id="inputListLimit">
		<By>xpath</By>
		<Address>//div[@id='list_limit_chzn']//input</Address>	
	</Element>
	<Element id="linkOrderingColumn">
		<By>cssSelector</By>
		<Address>a[data-original-title='Ordering']</Address>	
	</Element>

		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr/preceding-sibling::tr</Address>	
	</Element>
	<Element id="icStatusInTableWithTitle">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr/td[count(//table//th[contains(.,'Status')]/preceding-sibling::th)+1]/div/a[1]</Address>	
	</Element>
	<Element id="icStatusFeaturedInTableWithTitle">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr/td[count(//table//th[contains(.,'Status')]/preceding-sibling::th)+1]/div/a[2]</Address>	
	</Element>
	<Element id="icFeaturedInTableWithTitle">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr/td[count(//table//th[contains(.,'Status')]/preceding-sibling::th)+1]/div/a[2]/span[@class='icon-featured']</Address>	
	</Element>
	<Element id="icUnfeaturedInTableWithTitle">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr/td[count(//table//th[contains(.,'Status')]/preceding-sibling::th)+1]/div/a[2]/span[@class='icon-unfeatured']</Address>	
	</Element>
	<Element id="accessInTableWithTitle">
		<By>xpath</By>
		<Address>//table//a[normalize-space(text())='{0}']//ancestor::tr/td[count(//table//th[contains(.,'Access')]/preceding-sibling::th)+1]</Address>	
	</Element>

</Page>
